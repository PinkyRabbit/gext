{
  "manifest_version": 2,
  "name": "for sociotime.ru",
  "description": "Это расширение только для сайта sociotime.ru (only for sociotime.ru)",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://sociotime.ru/index.php*"
      ],
      "js": ["jquery.js", "content.js"]
    }
  ],
  "browser_action": {
    "name": "sociotime"
  }
}
