{
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "0.3",
  "default_locale": "ru",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://sociotime.ru/index.php",
        "https://sociotime.ru/index.php#"
      ],
      "js": ["jquery.js", "contentz.js"],
      "css": ["chromeext.css"]
    }
  ],
  "web_accessible_resources": [
    "injected.js"
  ],
  "browser_action": {
    "name": "sociotime",
    "default_icon": {
      "16": "/images/icon16.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  },
  "manifest_version": 2
}
